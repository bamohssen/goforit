<?php

namespace AppBundle\Entity;

/**
 * Invitation
 */
class Invitation
{
    /**
     * @var int
     */
    private $id;

    /**
     * @var string
     */
    private $description;

    private $utilisateur;


    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set description
     *
     * @param string $description
     *
     * @return Invitation
     */
    public function setDescription($description)
    {
        $this->description = $description;

        return $this;
    }

    /**
     * Get description
     *
     * @return string
     */
    public function getDescription()
    {
        return $this->description;
    }
}

